  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": " NODE_ENV=dev nest build --webpack --webpackPath webpack-hmr.config.js --watch ",
    "start:test": " NODE_ENV=test nest build --webpack --webpackPath -no-cache webpack-hmr.config.js --watch ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --detectOpenHandles --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --watch -no-cache --config ./test/jest-e2e.json --runInBand --detectOpenHandles",
